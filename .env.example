
# Google Calendar API Configuration
VITE_GOOGLE_API_KEY=your_google_api_key_here
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Apple Calendar Configuration (CalDAV)
VITE_APPLE_CALENDAR_SERVER=https://caldav.icloud.com
VITE_APPLE_CALENDAR_USERNAME=your_apple_id@icloud.com
VITE_APPLE_CALENDAR_PASSWORD=your_app_specific_password

# Calendar Sync Settings
VITE_CALENDAR_SYNC_INTERVAL=30
VITE_CALENDAR_AUTO_SYNC=true

# Debug logging
VITE_DEBUG=false

# Feature Flags
# Set to "true" to expose the login navigation item (VITE_ prefix required)
VITE_SHOW_LOGIN_MENU=false
